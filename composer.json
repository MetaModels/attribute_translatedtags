{
  "name": "metamodels/attribute_translatedtags",
  "description": "MetaModels attribute for translated tags",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "tags",
    "translatable"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_translatedtags/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_translatedtags"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev",
    "symfony/dependency-injection": "^3.3",
    "symfony/http-kernel": "^3.3",
    "metamodels/attribute_tags": "~2.1@dev"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.2",
    "contao/manager-plugin": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedTagsBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedTagsBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTranslatedTagsBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_translatedtags-",
        "languages_cto": "src/Resourcescontao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-feature/contao4": "2.1.x-dev"
    }
  },
  "prefer-stable": true
}
